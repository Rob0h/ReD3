{
  "extend": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "jsx-a11y",
    "react"
  ],
  "globals": {
    "document": true,
  },
  "rules": {
    "react/prop-types": "off",
    "import/prefer-default-export": "off",
    "no-plusplus": "off",
    "no-multi-spaces": "off",
    "no-unused-vars": "warn"
  }

}